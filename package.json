{
  "name": "gravity-bp",
  "version": "1.1.1",
  "description": "Gravity Boilerplate: A TypeScript powered React/Redux boilerplate by Jonny Asmar.",
  "main": "index",
  "scripts": {
    "start-api-dev": "nodemon --watch api --exec 'cross-env NODE_ENV=development sls offline start'",
    "start-api-prod": "cross-env NODE_ENV=production sls offline start",
    "build-api": "webpack --config webpack.config.api.js",
    "build-api-dev": "cross-env NODE_ENV=development npm run build-api",
    "build-api-prod": "cross-env NODE_ENV=production npm run build-api",
    "build-app": "webpack --config webpack.config.app.js",
    "serve-app": "webpack-serve --config webpack.config.app.js",
    "build-app-dev": "cross-env NODE_ENV=development npm run build-app",
    "build-app-prod": "cross-env NODE_ENV=production npm run build-app",
    "serve-app-dev": "cross-env NODE_ENV=development npm run serve-app",
    "serve-app-prod": "cross-env NODE_ENV=production npm run serve-app",
    "start": "npm run start-dev",
    "start-dev": "npm run build-api-dev && echo 'Starting...' & npm run start-api-dev & npm run serve-app-dev --watch & npm run build-api-dev --watch & npm run pgadmin",
    "start-prod": "npm run build-api-prod && echo 'Starting...' & npm run start-api-prod & npm run serve-app-prod",
    "deploy-api": "npm run build-api-prod && cross-env NODE_ENV=production sls deploy",
    "deploy-function": "npm run build-api-prod && cross-env NODE_ENV=production sls deploy function --function api",
    "deploy": "npm run build-app-prod && npm run deploy-api && sls syncToS3 && sls invalidateCloudFrontCache",
    "deploy-app": "npm run build-app-prod && sls syncToS3 && sls invalidateCloudFrontCache",
    "knex-migrate": "knex migrate:make --knexfile src/utils/rds.ts",
    "knex-rollback": "knex migrate:rollback --knexfile src/utils/rds.ts",
    "knex-update": "knex migrate:latest --knexfile src/utils/rds.ts",
    "precommit": "pretty-quick --staged",
    "pgadmin": "docker run -p 5050:5050 -v $INIT_CWD/pgadmin:/pgadmin thajeztah/pgadmin4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonnyasmar/gravity-bp.git"
  },
  "author": "Jonny Asmar",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@nest-middlewares/cors": "^1.1.1",
    "@nestjs/common": "^4.6.6",
    "@nestjs/core": "^4.6.6",
    "@nestjs/microservices": "^4.6.6",
    "@nestjs/testing": "^4.6.6",
    "@nestjs/websockets": "^4.6.6",
    "antd": "^3.6.1",
    "aws": "0.0.3-2",
    "aws-sdk": "^2.250.1",
    "aws-serverless-express": "^3.2.0",
    "axios": "^0.17.1",
    "babel-loader": "^7.1.4",
    "chunk-manifest-webpack-plugin": "^1.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "compression": "^1.7.2",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "faas-grip": "^1.0.1",
    "filewatcher-webpack-plugin": "^1.2.0",
    "glamor": "^2.20.40",
    "glamorous": "^4.13.1",
