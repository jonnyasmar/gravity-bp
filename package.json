{
  "name": "gravity-bp",
  "version": "1.1.1",
  "description": "Gravity Boilerplate: A TypeScript powered React/Redux boilerplate by Jonny Asmar.",
  "main": "index",
  "scripts": {
    "build": "webpack --config webpack.config.public.js",
    "_build": "NODE_ENV=production webpack --config webpack.config.public.js",
    "start":
      "webpack --config webpack.config.api.js && nodemon --exec 'sls offline start --port 8081' & webpack-dev-server --config webpack.config.public.js --hot --host 0.0.0.0 & webpack --watch --config webpack.config.api.js",
    "_start":
      "NODE_ENV=production webpack --config webpack.config.api.js && sls offline start --port 8081 & webpack-dev-server --config webpack.config.public.js --compress",
    "serve":
      "webpack --config webpack.config.public.js --config webpack.config.public.js --watch & ts-node ./src/utils/server.ts",
    "_serve": "NODE_ENV=production webpack --config webpack.config.public.js --watch & ts-node ./src/utils/server.ts",
    "_deploy": "NODE_ENV=production echo 'COMING SOON!'",
    "precommit": "pretty-quick --staged",
    "deploy": "webpack --config webpack.config.api.js && sls deploy",
    "sls": "webpack --config webpack.config.api.js && sls deploy function --function api"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonnyasmar/gravity-bp.git"
  },
  "author": "Jonny Asmar",
  "license": "MIT",
